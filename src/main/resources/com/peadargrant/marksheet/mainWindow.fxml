<AnchorPane id="AnchorPane" prefHeight="474.0" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.peadargrant.marksheet.MarkSheetController">
    <children>
      <MenuBar fx:id="menuBar" prefHeight="29.0" prefWidth="320.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <menus>
            <Menu fx:id="fileMenu" mnemonicParsing="false" text="File" />
        </menus>
      </MenuBar>

      <Label id="applicationLabel" fx:id="applicationLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="12.0" layoutY="29.0" prefHeight="44.0" prefWidth="292.0" text="Marking Sheet" textAlignment="CENTER" textFill="#000d6b">
         <font>
            <Font name="Helvetica Bold" size="32.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="73.0" mnemonicParsing="false" onAction="#chooseInputFile" prefHeight="26.0" prefWidth="292.0" text="Choose marking scheme file..." />
      <Label fx:id="inputFilenameLabel" layoutX="12.0" layoutY="108.0" prefHeight="16.0" prefWidth="292.0" text="/nofile" />
      <Button layoutX="14.0" layoutY="146.0" mnemonicParsing="false" onAction="#chooseOutputFile" prefHeight="26.0" prefWidth="292.0" text="Choose destination" />
      <Label fx:id="outputFilenameLabel" layoutX="14.0" layoutY="179.0" prefHeight="16.0" prefWidth="292.0" text="/nofile" />
      <Button layoutX="14.0" layoutY="211.0" mnemonicParsing="false" onAction="#generateExcelSheet" prefHeight="26.0" prefWidth="292.0" text="Generate Excel sheet" />
